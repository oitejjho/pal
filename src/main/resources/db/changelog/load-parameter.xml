<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.2.xsd">

    <changeSet id="load-parameter-data" author="Oitejjho">

        <preConditions onFailMessage="skipping load data, table not empty" onFail="MARK_RAN">
            <sqlCheck expectedResult="0">SELECT COUNT(*) FROM parameter</sqlCheck>
        </preConditions>

        <loadData file="data/parameters.csv" tableName="parameter">
            <column name="key" type="STRING"/>
            <column name="machine_key" type="STRING"/>
            <column name="name" type="STRING"/>
            <column name="type" type="STRING"/>
            <column name="unit" type="STRING"/>
        </loadData>

    </changeSet>

</databaseChangeLog>